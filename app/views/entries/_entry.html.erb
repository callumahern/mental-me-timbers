<div id="<%= dom_id entry %>">
  <p>
    <strong>Title:</strong>
    <%= entry.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= entry.description %>
  </p>

 <% @food.each do |entry| %>
    <%= link_to entry.food %>
  <% end %>

  <p>
    <strong>Date:</strong>
    <%= entry.start_time %>
  </p>

</div>
